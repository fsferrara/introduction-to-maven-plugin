{
  "name": "module-typescript-starter",
  "version": "1.0.0",
  "description": "Template module created with [this procedure](docs/template.md).",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "nodemon --config nodemon.json lib/index.ts",
    "dev:debug": "nodemon --config nodemon.json --inspect-brk lib/index.ts",
    "clean": "rimraf dist && rimraf .tsbuildinfo",
    "clean:modules": "rimraf node_modules",
    "test": "jest",
    "prepublish": "npm run build",
    "build": "tsc"
  },
  "keywords": [
    "node",
    "typescript",
    "template"
  ],
  "author": "fsferrara",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/fsferrara/sandbox#readme",
  "bugs": {
    "url": "https://github.com/fsferrara/sandbox/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/fsferrara/sandbox.git"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.10",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "rimraf": "3.0.2",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.0",
    "typescript": "^4.1.2"
  }
}
