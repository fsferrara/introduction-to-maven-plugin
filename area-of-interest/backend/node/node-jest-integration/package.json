{
  "name": "generic-template",
  "version": "1.0.0",
  "description": "node generic template with jest integration tests",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "test": "jest",
    "test:unit": "jest test/unit/*.test.js",
    "test:integration": "jest test/integration/*.test.js",
    "clean": "rimraf coverage",
    "clean:modules": "rimraf node_modules",
    "lint": "eslint \"**/*.js\" --ignore-pattern node_modules/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/fsferrara/sandbox.git"
  },
  "keywords": [
    "node",
    "javascript",
    "jest",
    "template"
  ],
  "author": "fsferrara",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fsferrara/sandbox/issues"
  },
  "homepage": "https://github.com/fsferrara/sandbox#readme",
  "devDependencies": {
    "eslint": "^8.31.0",
    "express": "^4.18.2",
    "jest": "^29.3.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.3"
  }
}
